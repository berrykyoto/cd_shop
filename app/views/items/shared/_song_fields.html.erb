<div class="nested-fields form-inline">
 <div class="form-group">
	
		<%= f.label :song_number, "曲順" %>
		<%= f.number_field :song_number, min: 1 %>
</div>
 <div class="form-group">
		<%= f.label :song_name, "曲名" %>
		<%= f.text_field :song_name %>
</div>
 <div class="form-group">
		<%= f.label :song_time, "時間" %>
		<%= f.text_field :song_time %>
</div>
</div>
