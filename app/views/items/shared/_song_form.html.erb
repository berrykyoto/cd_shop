 <%= tf.nested_fields_for :songs, wrapper_tag: :div do |r| %>
	<tr>
		<td><%= r.label :song_number, "曲順" %></td>
		<td><%= r.number_field :song_number, min: 1 %></td>

		<td><%= r.label :song_name, "曲名" %></td>
		<td><%= r.text_field :song_name %></td>

		<td><%= r.label :song_time, "時間" %></td>
		<td><%= r.text_field :song_time %></td>
	</tr>
			<%= r.hidden_field :id %>
	<tr>
		<td><%= r.remove_nested_fields_link 'Delete', class: 'btn btn-danger', role: 'button' %></td>
	</tr>
<% end %>
<%= tf.add_nested_fields_link :songs, 'Add song', class: 'btn btn-primary', role: 'button' %>